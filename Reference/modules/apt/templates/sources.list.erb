# Sources.list

<% if (lsbdistid.downcase == "debian") -%>
deb <%= source_url_real %> <%= lsbdistcodename %> <%= repos_real %>
deb <%= source_security_url %> <%= lsbdistcodename %>/updates <%= repos_real %>
<% end -%>

<% if (lsbdistid.downcase == "ubuntu") -%>
deb <%= source_url_real %> <%= lsbdistcodename %> <%= repos_real %>
deb <%= source_url_real %> <%= lsbdistcodename %>-updates <%= repos_real %>
deb <%= source_url_real %> <%= lsbdistcodename %>-security <%= repos_real %>
<% end -%>

<% if include_backports then -%>
deb <%= backports_url %> <%= lsbdistcodename %>-backports <%= repos_real %>
<% end -%>

<% if include_src then -%>
deb-src <%= source_url_real %> <%= lsbdistcodename %> <%= repos_real %>
deb-src <%= source_url_real %> <%= lsbdistcodename %>-updates <%= repos_real %>
deb-src <%= source_url_real %> <%= lsbdistcodename %>-security <%= repos_real %>
<% end -%>