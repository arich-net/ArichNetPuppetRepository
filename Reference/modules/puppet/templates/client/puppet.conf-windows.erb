#
# Puppet Agent configuration
#
[main]
pluginsync = true
autoflush=true

[master]
# This is a client only
ca = false

[agent]
certname = <%= puppet_certname %>
ca_server = <%= scope.lookupvar('puppet::params::ca_server') %>
server = <%= puppet_master %>
reportserver = <%= scope.lookupvar('puppet::params::report_server') %>
report = <%= puppet_report %>
configtimeout = 3600
runinterval = <%= scope.lookupvar('puppet::params::runinterval') %>
pluginsync = true
environment = <%= puppet_environment %>


