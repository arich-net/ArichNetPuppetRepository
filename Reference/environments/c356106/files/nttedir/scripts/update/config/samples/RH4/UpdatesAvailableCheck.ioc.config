ioc.services = logger,email,updates
logger.class = NTTEU::USF::Logger::RollingFileLogger
logger.argc = 2
logger.argv.1 = /etc/NTTEO/log/UpdatesAvailableCheck
logger.argv.2 = 5
email.class = NTTEU::USF::EMail::UnixSystemEMail
email.argc = 3
email.argv.1 = /bin/mail
email.argv.2 = /bin/mktemp
email.argv.3 = $svc{logger}
updates.class = NTTEU::USF::Updates::Up2DateUpdates
updates.argc = 2
updates.argv.1 = /usr/sbin/up2date
updates.argv.2 = $svc{logger}
