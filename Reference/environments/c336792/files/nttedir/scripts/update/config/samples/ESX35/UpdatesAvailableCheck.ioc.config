ioc.services = logger,email,updates
logger.class = NTTEU::USF::Logger::RollingFileLogger
logger.argc = 2
logger.argv.1 = /etc/NTTEO/log/UpdatesAvailableCheck
logger.argv.2 = 5
email.class = NTTEU::USF::EMail::NetSMTPEMail
email.argc = 2
email.argv.1 = autorelay.ntteo.net
email.argv.2 = $svc{logger}
updates.class = NTTEU::USF::Updates::ESXUpdates
updates.argc = 3
updates.argv.1 = /usr/sbin/esxupdate
updates.argv.2 = http://euw0800121.eu.verio.net:888/3.5.0/ 
updates.argv.3 = $svc{logger}
